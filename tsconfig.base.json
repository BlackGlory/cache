{
  "compilerOptions": {
    "target": "ES2018"
  , "module": "NodeNext"
  , "moduleResolution": "NodeNext"
  , "strict": true
  , "esModuleInterop": true
  , "skipLibCheck": true
  , "baseUrl": "."
  , "paths": {
      "@src/*": ["src/*"]
    , "@utils/*": ["src/utils/*"]
    , "@env/*": ["src/env/*"]
    , "@apis/*": ["src/apis/*"]
    , "@dao/*": ["src/dao/*"]
    , "@test/*": ["__tests__/*"]
    }
  }
}
