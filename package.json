{
  "private": true,
  "name": "cache",
  "version": "0.3.0",
  "type": "module",
  "repository": "git@github.com:BlackGlory/cache.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "cross-env NODE_OPTIONS='--experimental-vm-modules --loader ts-node/esm --inspect-brk' jest --runInBand --config jest.config.js",
    "test:coverage": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --coverage --config jest.config.js",
    "dev": "nodemon",
    "clean": "rimraf lib dist",
    "build": "tsc --project tsconfig.build.json",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag cache .",
    "docker:test": "docker-compose --file docker-compose.test.yml run --rm test",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test bundle smoke",
      "commit-msg": "commitlint --config commitlint.config.cjs --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.2.1",
    "@blackglory/wait-for": "^0.5.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^27.5.1",
    "@types/node": "16",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "husky": "4",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.1",
    "jest-resolve": "^28.1.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yarn-deduplicate": "^5.0.0"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.1.3",
    "@delight-rpc/websocket": "^0.5.4",
    "bufferutil": "^4.0.6",
    "delight-rpc": "^4.2.2",
    "dotenv": "^16.0.1",
    "extra-defer": "^0.2.3",
    "extra-disk-cache": "^0.8.0",
    "extra-filesystem": "^0.4.5",
    "extra-promise": "^2.3.0",
    "extra-timers": "^0.2.5",
    "iterable-operator": "^1.1.0",
    "justypes": "^2.0.1",
    "utf-8-validate": "^5.0.9",
    "value-getter": "^0.2.4",
    "ws": "^8.8.1",
    "you-died": "^0.4.1"
  }
}
